<Window x:Class="AutoCoder.WNamespaceManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoCoder"
        mc:Ignorable="d"
        Title="WNamespaceManager" Height="225" Width="800" SizeToContent="Height" WindowStyle="None">
    <Grid>
        <TextBlock x:Name="T_namespace" HorizontalAlignment="Left" Margin="10,14,0,0" TextWrapping="Wrap" Text="Namespaces" VerticalAlignment="Top" FontSize="18" Width="382"/>
        <ListBox x:Name="LB_nmsplist" HorizontalAlignment="Left" Height="159" Margin="10,43,0,0" VerticalAlignment="Top" Width="772" Background="#FFB0B0B0"/>
        <Button x:Name="B_add" Content="Add" HorizontalAlignment="Left" Margin="527,10,0,0" VerticalAlignment="Top" Width="125" FontSize="18"/>
        <Button x:Name="B_delete" Content="Delete" HorizontalAlignment="Left" Margin="657,10,0,0" VerticalAlignment="Top" Width="125" FontSize="18"/>
        <Button x:Name="B_close" Content="Close" HorizontalAlignment="Left" Margin="397,10,0,0" VerticalAlignment="Top" Width="125" FontSize="18" Click="BClicked"/>

    </Grid>
</Window>
