<Window x:Class="AutoCoder.ClassManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoCoder"
        mc:Ignorable="d"
        Title="ClassManagerWindow" Height="450" Width="800" Closing="WClosing">
    <Grid>
        <ListBox x:Name="LB_classes" HorizontalAlignment="Left" Height="154" Margin="10,255,0,0" VerticalAlignment="Top" Width="772"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,227,0,0" TextWrapping="Wrap" Text="Classes" VerticalAlignment="Top" FontSize="14" Height="23" Width="88"/>
        <Button x:Name="B_add" Content="Add" HorizontalAlignment="Left" Margin="547,230,0,0" VerticalAlignment="Top" Width="75" Click="B_Clicked"/>
        <Button x:Name="B_edit" Content="Edit" HorizontalAlignment="Left" Margin="627,230,0,0" VerticalAlignment="Top" Width="75" Click="B_Clicked"/>
        <Button x:Name="B_delete" Content="Delete" HorizontalAlignment="Left" Margin="707,230,0,0" VerticalAlignment="Top" Width="75" Click="B_Clicked"/>

    </Grid>
</Window>
