<Window x:Class="AutoCoder.WVarSetting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoCoder"
        mc:Ignorable="d"
        Title="WVarSetting" Height="450" Width="800" WindowStyle="None">
    <Grid>
        <Button x:Name="B_cancel" Content="Cancel" HorizontalAlignment="Left" Margin="578,398,0,0" VerticalAlignment="Top" Width="100" FontSize="18" Height="35" Click="Clicked"/>
        <Button x:Name="B_ok" Content="OK" HorizontalAlignment="Left" Margin="683,398,0,0" VerticalAlignment="Top" Width="100" FontSize="18" Height="35" Click="Clicked"/>
        <ListBox x:Name="LB_list" HorizontalAlignment="Left" Height="191" Margin="10,202,0,0" VerticalAlignment="Top" Width="773" FontSize="28">
        </ListBox>
        <TextBlock x:Name="T_subtitle" HorizontalAlignment="Left" Margin="10,165,0,0" TextWrapping="Wrap" Text="[1]subTitle" VerticalAlignment="Top" Width="773" FontSize="24"/>
        <TextBlock x:Name="T_title" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="[0]Title" VerticalAlignment="Top" Width="773" FontSize="24"/>

    </Grid>
</Window>
