<Window x:Class="AutoCoder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoCoder"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="B_New" Content="New" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Height="25" Click="B_Clicked"/>
        <Button x:Name="B_Save" Content="Save" HorizontalAlignment="Left" Margin="90,10,0,0" VerticalAlignment="Top" Width="75" Height="25" Click="B_Clicked"/>
        <Button x:Name="B_Load" Content="Load" HorizontalAlignment="Left" Margin="170,10,0,0" VerticalAlignment="Top" Width="75" Height="25" Click="B_Clicked"/>
        <Button x:Name="B_04de" Content="Delete Exec" HorizontalAlignment="Left" Margin="401,75,0,0" VerticalAlignment="Top" Width="381" Height="30" Click="B_Clicked"/>
        <ListBox x:Name="LB_02cf" HorizontalAlignment="Left" Height="114" Margin="10,295,0,0" VerticalAlignment="Top" Width="386"/>
        <ListBox x:Name="LB_01uf" HorizontalAlignment="Left" Height="114" Margin="10,176,0,0" VerticalAlignment="Top" Width="386"/>
        <ListBox x:Name="LB_03e" HorizontalAlignment="Left" Height="233" Margin="401,176,0,0" VerticalAlignment="Top" Width="381"/>
        <Button x:Name="B_02ae" Content="Add Exec.." HorizontalAlignment="Left" Margin="401,40,0,0" VerticalAlignment="Top" Width="381" Height="30" Click="B_Clicked"/>
        <Button x:Name="B_03fm" Content="Function Manager.." HorizontalAlignment="Left" Margin="10,75,0,0" VerticalAlignment="Top" Width="386" Height="30" Click="B_Clicked"/>
        <Button x:Name="B_01vm" Content="Variable Manager.." HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Width="386" Height="30" Click="B_Clicked"/>
        <ComboBox x:Name="CB_currentfunc" HorizontalAlignment="Left" Margin="401,149,0,0" VerticalAlignment="Top" Width="381" GotFocus="GetFocus"/>
        <ComboBox x:Name="CB_currentnamespace" HorizontalAlignment="Left" Margin="402,110,0,0" VerticalAlignment="Top" Width="381" ContextMenuOpening="CB_Opened" ContextMenuClosing="CB_Closed" GotFocus="GetFocus" Height="30"/>
        <Button x:Name="B_03nm" Content="Namespace Manager" HorizontalAlignment="Left" Margin="10,110,0,0" VerticalAlignment="Top" Width="386" Height="30" Click="B_Clicked"/>
    </Grid>
</Window>
